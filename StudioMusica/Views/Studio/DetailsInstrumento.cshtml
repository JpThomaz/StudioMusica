@model StudioMusica.Models.Instrumento

@{
    ViewData["Title"] = "DetailsInstrumento";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h1>DetailsInstrumento</h1>

<div>
    <h4>Instrumento</h4>
    <hr />
    <dl class="row">
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.Nome)
